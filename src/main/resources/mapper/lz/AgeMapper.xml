<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nk.lz.dao.AgeDAO">
    <select id="findAAll" resultType="com.nk.lz.domain.Age">
        select * from lz_age where lz_age.age is not NULL order by lz_age.age;
    </select>
</mapper>